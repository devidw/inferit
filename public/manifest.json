{
    "manifest_version": 3,
    "version": "2",
    "name": "inferit",
    "description": "Visual inference exploration & experimentation playground",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    },
    "icons": {
        "16": "icon_16.png",
        "24": "icon_24.png",
        "32": "icon_32.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "background": {
        "service_worker": "bg.js"
    },
    "action": {}
}